            <div class="col-lg-12">
                <h2>Enregistrement de vente</h2>
            </div>
            <div class="col-lg-12">
                {{ form_start(form) }}
                <div class="row">
                    <div class="col-lg-6">
                        {{ form_row(form.receive) }}
                        {{ form_row(form.taxe) }}
                        {{ form_row(form.taxe) }}
                    </div>
                    <div class="col-lg-6">
                        {{ form_row(form.commandes) }}                        
